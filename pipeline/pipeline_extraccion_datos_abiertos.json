{
	"name": "pipeline_extraccion_datos_abiertos",
	"properties": {
		"activities": [
			{
				"name": "CopiaDatos_Datalake",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DS_DatosGovColombia_JSON",
						"type": "DatasetReference",
						"parameters": {
							"where": "departamento_entidad = 'Meta'",
							"limit": "45000",
							"offset": "0"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DS_BlobStorage_DatosGov",
						"type": "DatasetReference",
						"parameters": {
							"path": "bronce",
							"file": "contratos.json"
						}
					}
				]
			}
		],
		"annotations": []
	}
}